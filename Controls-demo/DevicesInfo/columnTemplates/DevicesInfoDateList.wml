<ws:partial template="Controls/grid:ColumnTemplate" attr:class="js-controls-ListView__notEditable controls-Grid__row-cell__content_baseline_L" itemData="{{itemData}}">
    <ws:contentTemplate>
        <div class="DeviceInfo-dateItem-wrapper">
            <div class="DeviceInfo-year">{{itemData.item.format(itemData.item['ПоследнийВход'], itemData.item.format.FULL_DATE)}}</div>
           <div class="DeviceInfo-time">{{itemData.item.format(itemData.item['ПоследнийВход'], itemData.item.format.SHORT_TIME)}}</div>
        </div>
    </ws:contentTemplate>
</ws:partial>
